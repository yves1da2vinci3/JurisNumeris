       <h1 class="billContainer__date">
         <div class="clock__date">
        <!-- <span id="date-day-week"></span> -->
        <span id="date-day"></span>
        <span id="date-month"></span>
        <span id="date-year"></span>
    </div></h1>
     </div>
     <div class="billContainer__body">
        
   <h2 class="billContainer__body__title">informations</h2>

   <p class="informations__Email"> <span class="billContainer__body__info">Email : </span> <%= reservation.Email  %></p>
   <p class="informations__name"> <span class="billContainer__body__info">Nom : </span><%= reservation. userName %></p>
   <p class="informations__name"> <span class="billContainer__body__info">Prenom : </span> <%= reservation.LastName  %></p>
   <p class="informations__Numero"> <span class="billContainer__body__info">Numero : </span><%= reservation.userNumber %></p>
   <p class="informations__formation"> <span class="billContainer__body__info">Formation : </span><%= reservation.FormationName %></p>   
     </div>
     <div class="billContainer__Footer">
         <p class="Signature">SIGNATURE</p>
         <img class="SignatureImg" src="/images/Signature.png" />
     </div>
     <div class="navbar__container__btn">
        <div class="btn--login">
        Imprimer votre recu
        </div>
        
      </div>
    </div>
    <script type="text/javascript">
      dateDay = document.getElementById('date-day'),
      dateMonth = document.getElementById('date-month'),
      dateYear = document.getElementById('date-year')
  let date = new Date();
  let months = ['Jan', 'Fev', 'Mar', 'Apr', 'MaI', 'Juin', 'Juil', 'Aout', 'Sep', 'Oct', 'Nov', 'Dec']
    day = date.getDate(),
        // dayweek = date.getDay(),
        month = date.getMonth(),
        year = date.getFullYear()
        dateDay.innerHTML = day
    // dateWeek.innerHTML = `${week[dayweek]}`
    dateMonth.innerHTML = `${months[month]},`
    dateYear.innerHTML = year

</script>
